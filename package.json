{
	"name": "orc-scripts",
	"version": "0.4.0",
	"description": "Scripts toolbox for Orckestra",
	"bin": {
		"orc-scripts": "dist/index.js"
	},
	"engines": {
		"node": ">= 8",
		"npm": ">= 5"
	},
	"scripts": {
		"test": "node src test",
		"coverage": "node src test --coverage",
		"coveralls": "cat coverage/lcov.info | coveralls",
		"build": "node src build",
		"watch": "node src build --watch",
		"build:clean": "npm run clean & npm run build",
		"clean": "rm -r dist & mkdir dist",
		"precommit": "lint-staged",
		"prepublishOnly": "npm run build:clean"
	},
	"files": [
		"dist",
		"webpack.js",
		"jest.js",
		"eslint.js",
		"prettier.js"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Orckestra/orc-scripts.git"
	},
	"author": "Gert K. SÃ¸nderby <gert.sonderby@orckestra.com>",
