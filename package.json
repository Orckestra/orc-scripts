		"babel-preset-env": "1.7.0",
		"babel-preset-react-app": "3.1.2",
		"core-js": "2.5.7",
		"coveralls": "3.0.1",
		"cross-spawn": "6.0.5",
		"dotenv": "6.0.0",
		"eslint": "5.4.0",
		"eslint-config-react-app": "2.1.0",
		"eslint-plugin-flowtype": "2.50.0",
